<%= content_tag(:div, data: { controller: 'requests' }) do %>
  <%= content_tag(:ul) do %>
    <%= render(
      partial: 'to_dos/to_do',
      collection: list.to_dos.active.persisted
    ) %>
  <% end %>
  <%= render(
    partial: 'to_dos/new',
    locals: { list: list, new_to_do: new_to_do }
  ) %>
  <%= render(partial: 'archive_dones/new', locals: { list: list }) %>
<% end %>
